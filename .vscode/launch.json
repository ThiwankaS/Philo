{
	"version"			: "0.2.0",
	"configurations"	: [
			{
				"name"				: "Dinning Philosopher Debugger",
				"type"				: "cppdbg",
				"request"			: "launch",
				"program"			: "${workspaceFolder}/philo",
				"args"				: ["5", "800", "200" , "200", "3"],
				"stopAtEntry"		: false,
				"cwd"				: "${workspaceFolder}",
				"environment"		: [],
				"externalConsole"	: false,
				"MIMode"			: "gdb",
				"setupCommands"		: [
											{
												"description": "Enable pretty-printing for gdb",
												"text": "-enable-pretty-printing",
												"ignoreFailures": true
											}
									],
				"preLaunchTask"		: "Build Philosoper"
			}
	]
}
